#include <stdio.h>
#include <stdlib.h>

int main(){
    int num=1;
    while(num<=10){
        printf("Multiplication table for %d's \n", num);
        for(int i=1; i<=10; i++){
            printf("%d x %d = %d \n", num, i, num*i);
        }
        printf("===============================================\n");
        num++;
    }
    return 0;
}
